long minId = 100L
long maxId = 25000L

long minId2 = 25001L
long maxId2 = 49998L

// Generate random senderId and receiverId within range
def senderId = minId + (Math.random() * (maxId - minId + 1)) as long
def receiverId = minId2 + (Math.random() * (maxId2 - minId2 + 1)) as long

if (senderId == receiverId) {
  receiver++
}


// Generate random amount (positive long)
def amount = (Math.random() * 100).longValue() + 1  // customize upper limit

// Convert the UUID object to its string representation
def uuidString = UUID.randomUUID().toString()
requestId = uuidString

// Construct JSON payload
def payload = &quot;&quot;&quot;
{
  &quot;requestId&quot;: &quot;${requestId}&quot;,
  &quot;senderId&quot;: ${senderId},
  &quot;receiverId&quot;: ${receiverId},
  &quot;amount&quot;: 1
}
&quot;&quot;&quot;

